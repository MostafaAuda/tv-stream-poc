{
  "name": "etisalat-tv",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve -o",
    "build-dev": "ng build --configuration development",
    "build-prod": "ng build --configuration production",
    "watch": "ng build --watch --configuration development",
    "prebuild-prod-sam": "cd samsung/EtisalatTV && rimraf *.wgt *.js *.css *.html *.ico *.txt ./assets",
    "build-prod-sam": "npm run extract-ar-translation && npm run extract-en-translation && ng build --configuration production-sam",
    "postbuild-prod-sam": "move samsung\\EtisalatTV\\dist\\* samsung\\EtisalatTV && move samsung\\EtisalatTV\\dist\\assets samsung\\EtisalatTV",
    "prebuild-prod-lg": "cd lg/EtisalatTV && rimraf *.js *.css *.html *.ico *.txt ./assets",
    "build-prod-lg": "npm run extract-ar-translation && npm run extract-en-translation && ng build --configuration production-lg",
    "postbuild-prod-lg": "move lg\\EtisalatTV\\dist\\* lg\\EtisalatTV && move lg\\EtisalatTV\\dist\\assets lg\\EtisalatTV",
    "prebuild-dev-lg": "cd lg/EtisalatTV && rimraf *.js *.css *.html *.ico *.txt ./assets",
    "build-dev-lg": "npm run extract-ar-translation && npm run extract-en-translation && ng build --configuration dev-lg",
    "postbuild-dev-lg": "move lg\\EtisalatTV\\dist\\* lg\\EtisalatTV && move lg\\EtisalatTV\\dist\\assets lg\\EtisalatTV",
    "extract-ar-translation": "ngx-translate-extract --input  ./src --output -o ./src/assets/i18n/ar.json -k --clean --sort --format namespaced-json ",
    "extract-en-translation": "ngx-translate-extract --input  ./src --output -o ./src/assets/i18n/en.json  -k  --clean --sort --format namespaced-json ",
    "extract-trans": "npm run extract-ar-translation && npm run extract-en-translation",
    "installPackages": "npm i @ngx-translate/core @ngx-translate/http-loader @biesbjerg/ngx-translate-extract angular2-moment",
    "test": "ng test",
    "prettyVerify": "prettier --config .prettierrc.json --list-different src/app/",
    "prettyFix": "prettier --config .prettierrc.json --write src/app/"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.0",
    "@angular/common": "~12.2.0",
    "@angular/compiler": "~12.2.0",
    "@angular/core": "~12.2.0",
    "@angular/forms": "~12.2.0",
    "@angular/platform-browser": "~12.2.0",
    "@angular/platform-browser-dynamic": "~12.2.0",
    "@angular/router": "~12.2.0",
    "@biesbjerg/ngx-translate-extract": "^7.0.4",
    "@ng-bootstrap/ng-bootstrap": "^11.0.0",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "angular2-moment": "^1.9.0",
    "bootstrap": "^5.1.3",
    "js-search": "^2.0.0",
    "move-cli": "^2.0.0",
    "ngx-smart-modal": "^7.4.1",
    "ngx-spinner": "^13.0.0",
    "rimraf": "^3.0.2",
    "rxjs": "~6.6.0",
    "tslib": "^2.3.0",
    "video.js": "^7.17.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.16",
    "@angular/cli": "~12.2.16",
    "@angular/compiler-cli": "~12.2.0",
    "@angular/localize": "^12.1.1",
    "@types/jasmine": "~3.8.0",
    "@types/node": "^12.11.1",
    "@types/video.js": "^7.3.30",
    "jasmine-core": "~3.8.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "prettier": "2.6.2",
    "typescript": "~4.3.5"
  }
}
